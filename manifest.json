{
    "manifest_version": 2,
    "name": "Daily CS Comics (xkcd)",
    "description": "A webcomic of romance, sarcasm, math, and language.",
    "version": "1.1",
    "browser_action": {
        "default_icon": "tab.png",
        "default_popup": "alert.html"
    },
    "icons": { 
        "16": "tab.png",
        "48": "tab.png",
        "128": "tab.png" 
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "https://xkcd.com/info.0.json"
    ],
    "chrome_url_overrides": {
        "newtab": "page.html"
    }
}
